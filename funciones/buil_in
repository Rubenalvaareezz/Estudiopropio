numero = [4,5,7,1,37,100]

#Encontrando el numero mayor de una lista
numero_mas_alto = max(numero)

#Encontrando numero mas bajo de una lista
numero_mas_bajo = min(numero)

#redondeando a 6 decimales (siempre hacia abajo)
print(round(12,8383309))

#Retorna False -> 0, vacio, False, None \ True -> distinto a  0, True, cadena, datos no vacios
resultado_bool = bool()
#print(bool(""))

#Retorna True si todos los valores son verdaderos
resultado_all = all([244, "True",[233,234]])
print(resultado_all)

#Suma todos los valores de un iterable
print(sum(numero))
